@model IEnumerable<parcelPicUp.Models.ApplicationUser>

<h2>Users</h2>
<table class="table">
    <tr>
        <th>User Name</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>
                <a asp-action="Manage" asp-route-userId="@user.Id" class="btn btn-primary">Manage Roles</a>
            </td>
        </tr>
    }
</table>
